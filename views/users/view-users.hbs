      <article>
        
<h3 style="display: flex; justify-content: space-between; align-items: center; flex-direction: row-reverse;">
   <span>DANH SÁCH THÀNH VIÊN</span>
   <a href="/user" class="btn btn-primary">ĐĂNG KÍ THÀNH VIÊN</a>
</h3>

        <table class="table table-bordered">
          <tr>
            <th>USERNAME</th>
            <th>PASSWORD</th>
            <th>FULL NAME</th>
            <th>EMAIL</th>
            <th>PHONE</th>
            <th>ADDRESS</th>
            <th>ACTION</th>
          </tr>
          {{#each users}}
            <tr>
              <td>{{this.username}}</td>
              <td>{{this.password}}</td>
              <td>{{this.fullName}}</td>
              <td>{{this.email}}</td>
              <td>{{this.phone}}</td>
              <td>{{this.address}}</td>
              <td>
                <a
                  href="/user/edit/{{this._id}}"
                  class="btn btn-primary btn-sm"
                >Edit</a>
                <a
                  href="/user/delete/{{this._id}}"
                  onclick="return confirm('Are you sure?')"
                  class="btn btn-danger btn-sm"
                >Delete</a>
              </td>

            </tr>
          {{/each}}

        </table>

      </article>
